{
  "name": "dticket-range",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "import:stations": "node --env-file .env.local ./scripts/import-stations.mjs",
    "import:destinations": "rm -f ./data/destinations/* && node ./scripts/import.mjs",
    "import": "npm run import:stations && npm run import:destinations",
    "dev": "next dev",
    "build": "npm run import && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "engines": {
    "node": "20.x"
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.35",
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/google.maps": "^3.54.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "db-hafas": "^6.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5"
  }
}
